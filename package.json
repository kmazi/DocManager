{
  "name": "docmanager",
  "version": "1.0.0",
  "description": "A document manager that controls access rights and privileges",
  "main": "main.js",
  "scripts": {
    "test": "NODE_ENV=test jest --coverage -u && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:local": "NODE_ENV=development babel-node spec/run.js && yarn test:react -- -u",
    "test:react": "jest --coverage",
    "build": "webpack --config ./webpack.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "seed:db": "NODE_ENV=production sequelize db:migrate:undo:all  && sequelize db:migrate && sequelize db:seed:all",
    "start:dev": "nodemon --watch server --exec babel-node --presets es2015 -- main.js",
    "start": "npm run seed:db && babel-node main.js",
    "test:e2e": "nightwatch",
    "test:nyc": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 150000 --compilers js:babel-register ./server/test/**/*.js",
    "test:nyc:local": "nyc --reporter=html --reporter=text mocha --timeout 150000 --compilers js:babel-register ./server/test/**/*.js",
    "test:nyc:test": "NODE_ENV=test1 sequelize db:migrate && sequelize db:seed:all && nyc --reporter=html --reporter=text mocha --timeout 150000 --compilers js:babel-register ./server/test/**/*.js"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "server/test",
      "server/controller/role.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-mugochukwu/DocManager.git"
  },
  "keywords": [
    "document",
    "manager",
    "restriction",
    "owner",
    "private",
    "user",
    "public"
  ],
  "author": "Mazi Ugochukwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-mugochukwu/DocManager/issues"
  },
  "homepage": "https://github.com/andela-mugochukwu/DocManager#readme",
  "dependencies": {
    "amdefine": "^1.0.1",
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "history": "^4.6.3",
    "html-react-parser": "^0.3.5",
    "install": "^0.10.1",
    "jquery": "^3.2.1",
