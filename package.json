    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "postcss-loader": "^2.0.6",
    "postgresql": "^0.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
